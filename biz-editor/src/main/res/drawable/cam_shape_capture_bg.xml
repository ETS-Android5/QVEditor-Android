<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="oval">
      <stroke
        android:width="4dp"
        android:color="@android:color/white" />

      <size
        android:width="48dp"
        android:height="48dp" />

      <padding
        android:bottom="6dp"
        android:left="6dp"
        android:right="6dp"
        android:top="6dp" />

    </shape>
  </item>

  <item>
    <shape android:shape="oval">

      <solid android:color="@android:color/white" />

    </shape>
  </item>

</layer-list>
